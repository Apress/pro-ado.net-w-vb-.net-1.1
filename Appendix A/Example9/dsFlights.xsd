<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="dsFlights" targetNamespace="http://tempuri.org/dsFlights.xsd" elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/dsFlights.xsd" xmlns:mstns="http://tempuri.org/dsFlights.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="dsFlights" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="tabFlights">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="FLIGHTCODE" type="xs:string" />
							<xs:element name="LeaveFrom" type="xs:string" minOccurs="0" />
							<xs:element name="GoingTo" type="xs:string" minOccurs="0" />
							<xs:element name="Depart" type="xs:dateTime" minOccurs="0" />
							<xs:element name="Arrive" type="xs:dateTime" minOccurs="0" />
							<xs:element name="FirstName" type="xs:string" minOccurs="0" />
							<xs:element name="LastName" type="xs:string" minOccurs="0" />
							<xs:element name="ID_PILOT" msdata:AutoIncrement="true" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>